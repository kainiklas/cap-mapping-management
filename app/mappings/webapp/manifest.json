{
    "_version": "1.8.0",
    "sap.app": {
        "id": "mappings",
        "type": "application",
        "title": "Manage Mappings",
        "description": "Manage Mappings",
        "i18n": "i18n/i18n.properties",
        "dataSources": {
            "MappingService": {
                "uri": "/mapping/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        }
    },
    "sap.ui5": {
        "dependencies": {
            "libs": {
                "sap.fe": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "": {
                "dataSource": "MappingService",
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect" : true,
                    "earlyRequests": true,
                    "groupProperties": {
                        "default": {
                            "submit": "Auto"
                        }
                    }
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "MappingsList",
                    "target": "MappingsList"
                },
                {
                    "pattern": "Mappings({key}):?query:",
                    "name": "MappingsDetails",
                    "target": "MappingsDetails"
                },
                {
                    "pattern": "Mappings({key}/category({key2}):?query:",
                    "name": "CategoriesDetails",
                    "target": "CategoriesDetails"
                }
            ],
            "targets": {
                "MappingsList": {
                    "type": "Component",
                    "id": "MappingsList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings" : {
                            "entitySet" : "Mappings",
                            "navigation" : {
                                "Mappings" : {
                                    "detail" : {
                                        "route" : "MappingsDetails"
                                    }
                                }
                            }
                        }
                    }
                },
                "MappingsDetails": {
                    "type": "Component",
                    "id": "MappingsDetailsList",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings" : {
                            "entitySet" : "Mappings",
                            "navigation" : {
                                "Categories" : {
                                    "detail" : {
                                        "route" : "CategoriesDetails"
                                    }
                                }
                            }
                        }
                    }
                },
                "CategoriesDetails": {
                    "type": "Component",
                    "id": "CategoriesDetailsList",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings" : {
                            "entitySet" : "Categories"
                        }
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "fullWidth": false
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    },
    "sap.ui.generic.app": {
		"_version": "1.3.0",
		"settings": {
			"forceGlobalRefresh": false,
			"objectPageHeaderType": "Dynamic",
			"showDraftToggle": false
        }
    }
}